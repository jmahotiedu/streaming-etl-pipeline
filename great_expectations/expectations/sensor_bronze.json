      "kwargs": {
        "column": "sensor_id",
        "regex": "^sensor-\\d{3}$"
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "value",
        "mostly": 0.99
      },
      "meta": {"notes": "Allow up to 1% null values"}
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "location",
        "mostly": 0.99
      },
      "meta": {}
